{
    "name": "hjelseth-computers/bulksms-notification-channel",
    "description": "BulkSMS notification channel for laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "notifications", "bulksms"],
    "authors": [
        {
            "name": "Osama Aldemeery",
            "email": "aldemeery@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.3",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/notifications": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Aldemeery\\BulkSMS\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aldemeery\\BulkSMS\\BulkSMSChannelServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
